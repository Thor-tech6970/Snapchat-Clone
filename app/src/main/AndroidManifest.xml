<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.SnapchatClone">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SnapchatClone.SnapsActivity"></activity>
        <activity android:name="com.example.SnapchatClone.userList" />
        <activity android:name="com.example.SnapchatClone.MainActivity" />
        <activity android:name="com.example.SnapchatClone.CreateSnapActivity" />
        <activity android:name="com.example.SnapchatClone.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>